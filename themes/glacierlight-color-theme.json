  "semanticTokenColors": {
    // General
    "enumMember": { "fontStyle": "", "foreground": "#303038" },
    "namespace": { "fontStyle": "italic", "foreground": "#505058" },

    // Regex
    "regexGrouping": "#9696a0",
    "regexText": "#606068",
    "regexAlternation": "#606068",
    "regexQuantifier": "#588dbe",

    // python
    "module:python": { "fontStyle": "bold", "foreground": "#2d75ba" },

    // Toml
    "tomlArrayKey": { "fontStyle": "", "foreground": "#303038" }
  },

  "tokenColors": [
    // General
    {
      "name": "Comments",
      "scope": [
        "comment",
        "punctuation.definition.comment",
        "comment.documentation",
        "comment.block.documentation"
      ],
      "settings": {
        "foreground": "#84848a"
      }
    },
    {
      "name": "Invalid - Illegal",
      "scope": "invalid.illegal",
      "settings": {
        "foreground": "#bd1414"
      }
    },
    {
      "name": "Operators",
      "scope": "keyword.operator",
      "settings": {
        "foreground": "#303038"
      }
    },
    {
      "name": "Keywords",
      "scope": ["keyword", "storage", "entity.other.attribute-name"],
      "settings": {
        "foreground": "#1b6ab4"
      }
    },
    {
      "name": "Keywords",
      "scope": [
        "meta.property-name",
        "support.type.property-name",
        "storage.type",
        "support.type"
      ],
      "settings": {
        "fontStyle": "",
        "foreground": "#1b6ab4"
      }
    },
    {
      "name": "Language Constants",
      "scope": ["constant.language", "support.constant", "variable.language"],
      "settings": {
        "foreground": "#1b6ab4"
      }
    },
    {
      "name": "Variables",
      "scope": ["variable", "support.variable"],
      "settings": {}
    },
    {
      "name": "Functions",
      "scope": ["entity.name.function", "support.function"],
      "settings": {}
    },
    {
      "name": "Classes",
      "scope": [
        "entity.name.type",
        "entity.name.tag",
        "entity.name.section",
        "entity.other.inherited-class",
        "support.class",
        "keyword.type",
        "storage.type.var",
        "meta.selector",
        "meta.selector entity",
        "meta.selector entity punctuation",
        "meta.tag.sgml.doctype",
        "meta.tag.sgml.doctype string",
        "meta.tag.sgml.doctype entity.name.tag",
        "meta.tag.sgml punctuation.definition.tag.html",
        "meta.diff.range",
        "meta.diff.index",
        "meta.separator"
      ],
      "settings": {
        "fontStyle": "bold",
        "foreground": "#2d75ba"
      }
    },
    {
      "name": "Exceptions",
      "scope": "entity.name.exception",
      "settings": {
        "foreground": "#bd1414"
      }
    },
    {
      "name": "Strings",
      "scope": "string",
      "settings": {
        "fontStyle": "bold",
        "foreground": "#6262a1"
      }
    },
    {
      "name": "meta template expression",
      "scope": ["meta.interpolation.cs", "meta.fstring.python"],
      "settings": {
        "fontStyle": "bold",
        "foreground": "#588dbe"
      }
    },
    {
      "name": "Strings: Escape Sequences",
      "scope": [
        "constant.character.escape",
        "constant.character.entity",
        "constant.other.symbol",
        "punctuation.definition.entity"
      ],
      "settings": {
        "foreground": "#588dbe"
      }
    },

    // Markup
    {
      "name": "Markup Heading",
      "scope": "markup.heading",
      "settings": {
        "foreground": "#1b6ab4"
      }
    },
    {
      "name": "Markup: Underline",
      "scope": "markup.underline",
      "settings": {
        "fontStyle": "underline"
      }
    },
    {
      "name": "Markup Quote",
      "scope": "markup.quote",
      "settings": {
        "fontStyle": "italic"
      }
    },
    {
      "name": "Markup: Insertion",
      "scope": ["markup.inserted", "punctuation.definition.to-file.diff"],
      "settings": {
        "foreground": "#168196"
      }
    },
    {
      "name": "Markup: Deletion",
      "scope": ["markup.deleted", "punctuation.definition.from-file.diff"],
      "settings": {
        "foreground": "#bb39c0"
      }
    },
    {
      "name": "Markup: Changed",
      "scope": "markup.changed",
      "settings": {
        "foreground": "#1b6ab4"
      }
    },
    {
      "name": "Markup: Link",
      "scope": ["meta.link", "meta.image.inline"],
      "settings": {
        "foreground": "#4a47ea"
      }
    },
    {
      "name": "Markup: Output",
      "scope": ["markup.output", "markup.raw"],
      "settings": {
        "foreground": "#777777"
      }
    },
    {
      "name": "Markup: Traceback",
      "scope": "markup.traceback",
      "settings": {
        "foreground": "#bd1414"
      }
    },
    {
      "name": "Markup: Error",
      "scope": "markup.error",
      "settings": {
        "foreground": "#bd1414"
      }
    },
    {
      "name": "Markup: Prompt",
      "scope": "markup.prompt",
      "settings": {
        "foreground": "#777777"
      }
    },

    // csharp
    {
      "name": "CSharp Directives",
      "scope": [
        "keyword.other.directive.using.cs",
        "keyword.other.directive.static.cs",
        "storage.type.namespace.cs"
      ],
      "settings": {
        "fontStyle": "italic"
      }
    },
    {
      "name": "CSharp Directives",
      "scope": ["entity.name.type.namespace.cs"],
      "settings": {
        "fontStyle": "italic",
        "foreground": "#505058"
      }
    },

    // Python
    {
      "name": "Pythoon Directives",
      "scope": ["keyword.control.import.python"],
      "settings": {
        "fontStyle": "italic"
      }
    },

    // HTML/CSS
    {
      "name": "CSS Property Name",
      "scope": [
        "support.type.property-name.css",
        "support.type.vendored.property-name.css",
        "support.type.property-name.json.comments"
      ],
      "settings": {
        "fontStyle": "",
        "foreground": "#303038"
      }
    },
    {
      "name": "CSS: Important Keyword",
      "scope": "keyword.other.important",
      "settings": {
        "fontStyle": "bold"
      }
    },

    // Xaml
    {
      "name": "Xaml tag name",
      "scope": [
        "entity.name.tag.localname.xml",
        "entity.name.tag.namespace.xml"
      ],
      "settings": {
        "fontStyle": "",
        "foreground": "#303038"
      }
    },

    // Markdown
    {
      "name": "Markdown Lists",
      "scope": [
        "punctuation.definition.markdown",
        "punctuation.definition.heading.markdown",
        "punctuation.definition.metadata.markdown",
        "punctuation.definition.raw.markdown",
        "punctuation.definition.bold.markdown",
        "punctuation.definition.italic.markdown",
        "punctuation.definition.strikethrough.markdown",
        "punctuation.definition.list.begin.markdown",
        "punctuation.definition.quote.begin.markdown",
        "punctuation.definition.link.title.begin.markdown",
        "punctuation.definition.link.title.end.markdown",
        "punctuation.definition.link.description.begin.markdown",
        "punctuation.definition.link.description.end.markdown",
        "punctuation.definition.table.markdown",
        "punctuation.separator.table.markdown",
        "fenced_code.block.language"
      ],
      "settings": {
        "fontStyle": "bold",
        "foreground": "#4B83CD"
      }
    },
    {
      "name": "Markdown Link Title",
      "scope": [
        "string.other.link.title.markdown",
        "string.other.link.description.markdown"
      ],
      "settings": {
        "fontStyle": "",
        "foreground": "#303038"
      }
    },

    // Toml
    {
      "name": "Property Names",
      "scope": ["support.type.property-name.toml"],
      "settings": {
        "fontStyle": "",
        "foreground": "#303038"
      }
    }
  ]
}
